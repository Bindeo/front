    <div>
        <input type="text" class="form-control" name="name" id="field-name" placeholder="(e.g. Shareholder's agreement)">

        <span class="glyphicon glyphicon-remove form-control-feedback" aria-hidden="true"></span>

        <span id="" class="help-block">Introduce una dirección de correo válida</span>
    </div>
</div>
#}
{{ form_widget(form.path) }}
<button type="submit" class="btn btn-default">{% trans %}Register ownership{% endtrans %}</button>
{{ form_end(form) }}
